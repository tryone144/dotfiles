Xcursor.theme: Oxygen Neon

# interface
URxvt*.iconFile: terminal
URxvt*.depth: 32
URxvt*.font: xft:ubuntu mono:size=12
URxvt*.boldFont: xft:ubuntu mono:bold:size=12
URxvt*.geometry: 132x43
URxvt*.scrollstyle: plain
URxvt*.scrollBar_right: true
URxvt*.secondaryScroll: true
URxvt*.scrollTtyOutput: false
URxvt*.scrollWithBuffer: true
URxvt*.scrollTtyKeypress: true
URxvt*.print-pipe: "cat > ~/.urxvt/$(date).dump"
URxvt*.cursorBlink: true
URxvt*.per-ext-common: default,tabbedex,matcher,vtwheel
URxvt*.urlLoader: chromium
URxvt*.tabbed.tabbar-fb: 2
URxvt*.tabbed.tabbar-bg: 0
URxvt*.tabbed.tab-fg: 3
URxvt*.tabbed.tab-bg: 0

# colors
URxvt*.foreground: #00FF00
URxvt*.background: [90]#000000
URxvt*.color0: #000000
URxvt*.color1: #CD0000
URxvt*.color2: #00CD00
URxvt*.color3: #CDCD00
URxvt*.color4: #1E90FF
URxvt*.color5: #CD00CD
URxvt*.color6: #00CDCD
URxvt*.color7: #E5E5E5
URxvt*.color8: #4C4C4C
URxvt*.color9: #FF0000
URxvt*.color10: #00FF00
URxvt*.color11: #FFFF00
URxvt*.color12: #4682B4
URxvt*.color13: #FF00FF
URxvt*.color14: #00FFFF
URxvt*.color15: #FFFFFF

